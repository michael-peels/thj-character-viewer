{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate": "npx openapi-typescript-codegen --input http://localhost:8080/v3/api-docs --output ./src/generated/ --useOptions && npm run openapi:set-port",
    "openapi:set-port": "sed -i '' -e \"s/8080/3000/\" src/generated/core/OpenAPI.ts"
  },
  "dependencies": {
    "openapi-typescript-codegen": "^0.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "sass": "^1.80.5",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
